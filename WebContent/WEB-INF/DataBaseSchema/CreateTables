USE mysql;

create table project(
  id int not null,
  name varchar (90) not null,
  CNPJCPF varchar (15) not null,
  primary key (id),
  unique key (CNPJCPF,name),
  foreign key (CNPJCPF) references client(CNPJCPF)
);

create table client(
  CNPJCPF varchar (15) not null,
  name varchar (90) not null,
  type varchar (1) not null,
  primary key (CNPJCPF)
);